classDiagram
    class Template {
        +execute()
        +callback: Callback
    }

    class Callback {
        <<interface>>
        +call()
    }

    class BusinessLogic1 {
        +call()
    }

    class BusinessLogic2 {
        +call()
    }

    Template --> Callback : uses
    BusinessLogic1 ..|> Callback
    BusinessLogic2 ..|> Callback
